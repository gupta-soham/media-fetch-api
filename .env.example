# Media Fetch API Configuration
HOST=0.0.0.0
PORT=8000
DEBUG=false

# Cookie directory path (relative to project root)
COOKIE_DIR=./cookies

# FFmpeg binary path (leave empty for system default)
FFMPEG_PATH=

# Request timeout in seconds
REQUEST_TIMEOUT=30

# Max retries for failed requests
MAX_RETRIES=3

# User-Agent string (leave empty for default)
USER_AGENT=

# CORS allowed origins (comma-separated). Empty = allow all origins without credentials.
# Example: https://app.example.com,https://www.example.com
CORS_ORIGINS=
